<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<!-- Enable/Disable Tweak -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Enable Tweak</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>isEnabled</string>
		</dict>
		<!-- Appearance -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Appearance</string>
			<key>footerText</key>
			<string>Default alpha value is 0.80.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Custom Note Color</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>useCustomNoteColor</string>
			<key>id</key>
			<string>useCustomNoteColorCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>SNPColorPickerController</string>
			<key>label</key>
			<string>Note Color</string>
			<key>id</key>
			<string>noteColorCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Custom Font Color</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>useCustomFontColor</string>
			<key>id</key>
			<string>useCustomFontColorCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>SNPColorPickerController</string>
			<key>label</key>
			<string>Font Color</string>
			<key>id</key>
			<string>fontColorCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Custom Alpha</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>useCustomAlpha</string>
			<key>id</key>
			<string>useCustomAlphaCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSliderCell</string>
			<key>min</key>
			<real>0.0</real>
			<key>max</key>
			<real>1.0</real>
			<key>default</key>
			<real>0.8</real>
			<key>showValue</key>
			<true/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>alphaValue</string>
			<key>id</key>
			<string>alphaCell</string>
		</dict>
		<!-- Sizing -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Sizing</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Font Size:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>20</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>fontSize</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Corner Radius:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>5</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>cornerRadius</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Width:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>250</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>width</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Height:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>250</string>
			<key>bestGuess</key>
			<string>250</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>height</string>
		</dict>
		<!-- Show/Hide Button -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Show/Hide Button</string>
			<key>footerText</key>
        	<string>Tip: Long press on the show/hide button to re-center the note.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>X-Axis Offset:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>0</string>
			<key>bestGuess</key>
			<string>0</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>xOffset</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>label</key>
			<string>Y-Axis Offset:</string>
			<key>isNumeric</key>
			<true/>
			<key>placeholder</key>
			<string>0</string>
			<key>bestGuess</key>
			<string>0</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>yOffset</string>
		</dict>
		<!-- Animations -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Animations</string>
			<key>footerText</key>
			<string>Default duration for the hide animation is 0.30 seconds.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkListCell</string>
			<key>detail</key>
			<string>PSListItemsController</string>
			<key>label</key>
			<string>Show/Hide Animation</string>
			<key>validTitles</key>
			<array>
				<string>Fade</string>
				<string>Curl</string>
				<string>Flip from Left</string>
				<string>Flip from Right</string>
				<string>Flip from Top</string>
				<string>Flip from Bottom</string>
			</array>
			<key>validValues</key>
			<array>
				<string>0</string>
				<string>1</string>
				<string>2</string>
				<string>3</string>
				<string>4</string>
				<string>5</string>
			</array>
			<key>default</key>
			<string>Fade</string>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>animationType</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Custom animation duration</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>useCustomDuration</string>
			<key>id</key>
			<string>useCustomDurationCell</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSliderCell</string>
			<key>min</key>
			<real>0.0</real>
			<key>max</key>
			<real>5.0</real>
			<key>default</key>
			<real>0.3</real>
			<key>showValue</key>
			<true/>
			<key>defaults</key>
			<string>com.gabrielsiu.stickynoteprefs</string>
			<key>key</key>
			<string>customDuration</string>
			<key>id</key>
			<string>durationCell</string>
		</dict>
		<!-- About/Acknowledgements -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>About</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>SNPAcknowledgementsController</string>
			<key>label</key>
			<string>Acknowledgements</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>@gabrielsiu_dev</string>
			<key>action</key>
			<string>openTwitter</string>
		</dict>
	</array>
	<key>title</key>
	<string>StickyNote</string>
</dict>
</plist>
